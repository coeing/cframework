<?xml version="1.0" encoding="utf-8"?>
<window xmlns="http://cframework.oeing.eu/schemas/ctrls"
   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
   xsi:schemaLocation="http://cframework.oeing.eu/schemas/ctrls ../../common/ctrls/window.xsd"
   width="640" height="242">
  <image size_mode="stretch">
    <state_normal texture="data/art/windows/NewsStudioWindow/bg.png"/>
  </image>
  <checkbox name="BtNewsChannel1" offset_x="8" offset_y="150" width="32" height="48" bg_visible="false">
    <button name="theButton" size_mode="stretch" bg_visible="false" button_type="toggle">
      <image width="32" height="40" bg_visible="false">
        <state_normal texture="data/art/windows/NewsStudioWindow/button_channel_political.png" />
        <state_highlight bg_color="0xccccccff" />
        <state_active_highlight bg_color="0xccccccff" />
      </image>
      <image offset_x="4" offset_y="36" width="16" height="12" bg_visible="false">
        <state_normal texture="data/art/windows/NewsStudioWindow/channel_light_off.png" />
        <state_active texture="data/art/windows/NewsStudioWindow/channel_light_on.png" />
      </image>
    </button>
  </checkbox>
  <checkbox name="BtNewsChannel2" offset_x="42" offset_y="150" width="40" height="48" bg_visible="false">
    <button name="theButton" size_mode="stretch" bg_visible="false" button_type="toggle">
      <image width="40" height="40" bg_visible="false">
        <state_normal texture="data/art/windows/NewsStudioWindow/button_channel_show.png" />
        <state_highlight bg_color="0xccccccff" />
        <state_active_highlight bg_color="0xccccccff" />
      </image>
      <image reference_attach_point="bottom" local_attach_point="bottom" width="16" height="12" bg_visible="false">
        <state_normal texture="data/art/windows/NewsStudioWindow/channel_light_off.png" />
        <state_active texture="data/art/windows/NewsStudioWindow/channel_light_on.png" />
      </image>
    </button>
  </checkbox>
  <checkbox name="BtNewsChannel3" offset_x="84" offset_y="150" width="40" height="48" bg_visible="false">
    <button name="theButton" size_mode="stretch" bg_visible="false" button_type="toggle">
      <image width="40" height="40" bg_visible="false">
        <state_normal texture="data/art/windows/NewsStudioWindow/button_channel_sundry.png" />
        <state_highlight bg_color="0xccccccff" />
        <state_active_highlight bg_color="0xccccccff" />
      </image>
      <image reference_attach_point="bottom" local_attach_point="bottom" width="16" height="12" bg_visible="false">
        <state_normal texture="data/art/windows/NewsStudioWindow/channel_light_off.png" />
        <state_active texture="data/art/windows/NewsStudioWindow/channel_light_on.png" />
      </image>
    </button>
  </checkbox>
  <panel name="ObjBoard" offset_x="136" offset_y="16" width="156" height="122" bg_visible="false">
  </panel>
</window>





