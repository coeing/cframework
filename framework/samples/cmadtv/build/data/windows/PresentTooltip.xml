      <textbox name="TbGiven" attach_object="prev sibling" reference_attach_point="bottom left" size_mode_x="stretch" size_mode_y="content" bg_visible="false" padding_left="4" padding_right="4" text="(1. time)" />
    </panel>
  </panel>
</panel>





