<?xml version="1.0" encoding="utf-8"?>
<treenode_style xmlns="http://cframework.oeing.eu/schemas/ctrls/styles"
   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
   xsi:schemaLocation="http://cframework.oeing.eu/schemas/ctrls/styles ../treenode_style.xsd"
   size_mode_x="stretch" height="18">
  <button name="theButton" offset_x="5" offset_y="5" width="9" height="9" button_type="toggle">
    <image size_mode="stretch">
      <state_normal texture="common/art/ctrls/windows/buttons/wbuttons.png" texture_x="0" texture_y="0" texture_width="9" texture_height="9" />
      <state_active texture="common/art/ctrls/windows/buttons/wtree_minus.png" texture_x="0" texture_y="0" texture_width="9" texture_height="9" />
    </image>
  </button>
  <textbox name="theTextBox" size_mode_x="content" offset_x="31" height="18" text="Desktop" bg_visible="true" font_size="10">
    <state_highlight text_color="0x0066ccff" underline="true" />
    <state_active bg_color="0x0a246aff" text_color="0xffffffff" />
    <state_active_highlight underline="true" />
  </textbox>
</treenode_style>





